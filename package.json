{
  "name": "storybook-addon-theme-playground",
  "version": "2.2.0",
  "description": "Select between themes and tweak them directly in a panel.",
  "keywords": [
    "storybook-addons",
    "storybook-addon",
    "storybook",
    "addon",
    "theme",
    "playground",
    "react",
    "typescript",
    "style"
  ],
  "storybook": {
    "displayName": "Theme Playground",
    "supportedFrameworks": [
      "React"
    ]
  },
  "bugs": "https://github.com/jeslage/storybook-addon-theme-playground/issues",
  "repository": "jeslage/storybook-addon-theme-playground",
  "license": "MIT",
  "author": "Johannes Eslage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p .",
    "commit": "cz",
    "build:watch": "npm run clean && tsc -p . -w",
    "clean": "rimraf dist",
    "deploy-example": "cd example && npm run deploy",
    "lint": "eslint ./src --ext ts --ext tsx",
    "prepare": "npm run build",
    "prettier-write": "prettier --write \"src/**/*.tsx\"",
    "prepublish": "npm run prettier-write && npm run lint && npm run type-check",
    "test": "echo \"No test specified\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm rum lint",
      "npm rum prettier-write"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "@storybook/api": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/client-api": "^7.0.2",
    "@storybook/client-logger": "^7.0.2",
    "@storybook/components": "^7.0.2",
    "@storybook/manager-api": "^7.0.2",
    "@storybook/preview-api": "^7.0.2",
    "@storybook/theming": "^7.0.2",
    "lodash.startcase": "^4.4.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@storybook/react": "^7.0.2",
    "@types/node": "^20.6.0",
    "@types/react": "18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "react": "^18.0.1",
    "react-dom": "^18.0.1",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.6",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@storybook/react": "^7.0.2",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  }
}
